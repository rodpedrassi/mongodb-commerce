// 16 - Adicione o campo ultimaModificacao com a data corrente somente no sandu√≠che Big Mac

use("commerce");
db.produtos.updateMany(
    { nome: { $eq: "Big Mac" } },
    { $currentDate: { ultimaModificacao: true } },
);

db.produtos.find(
  { ultimaModificacao: { $exists: true } },
  { nome: 1, _id: 0 },
);
